{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "Node16",
    "moduleResolution": "Node16",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": false,
    "outDir": "dist-electron",
    "target": "ES2020",
    "types": ["node"]
  },
  "include": ["vite.config.ts", "electron/**/*", "package.json"]
}

